{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Uvicorn",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/bin/uvicorn",
            "args": [
                "app.main:app",
                "--reload"
            ],
            "env": {
                "RESET_DB": "true"
            },
            "console": "integratedTerminal"
        }
    ]
}